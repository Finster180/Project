<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jwt.hibernate.bean.User" table="user">
		<id column="ID" name="id" type="java.lang.Integer" />
		<property column="USER_NAME" name="username" type="java.lang.String" />
		<property column="Address" name="Address" type="string" />
		<property column="EMAIL" name="email" type="java.lang.String" />
		<property column="Dob" name="Dob" type="java.lang.String" />
		<property column="Services" name="Services" type="java.lang.String" />
	</class>
</hibernate-mapping>
